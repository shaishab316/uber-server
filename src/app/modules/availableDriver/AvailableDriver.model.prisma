model AvailableDriver {
  id String @id @default(auto()) @map("_id") @db.ObjectId

  driver_id String    @unique @db.ObjectId
  driver    User      @relation(fields: [driver_id], references: [id], onDelete: Cascade)
  location  TLocation
  vehicle   EVehicle
  trip_id   String?   @db.ObjectId

  @@map("available_drivers")
}
