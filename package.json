  "name": "uber",
  "version": "1.0.1",
  "description": "A driver management platform for Uber-like services",
  "main": "dist/server.js",
  "author": {
    "name": "Shaishab Chandra Shil",
    "github": "https://github.com/shaishab316"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Alpha-Bytes-Department/uber-server.git"
  },
  "scripts": {
    "new-module": "node ./src/app/modules/.builder.js",
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "set DEBUG=socket.io* && ts-node-dev --respawn --transpile-only --watch .env src/server.ts",
    "stripe": "npx ts-node scripts/stripe.ts",
    "lint": "eslint src --ignore-path .gitignore --ext .ts",
    "prettier": "prettier --ignore-path .gitignore --write \"./src/**/*.+(js|ts|json)\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prisma:studio": "npx prisma studio",
    "prisma:generate": "npx prisma generate",
    "prisma:push": "npx prisma db push",
    "prisma:cleanup": "rm -rf ./prisma"
  },
  "keywords": [
    "uber",
    "driver",
    "management",
    "platform"
  ],
  "license": "MIT",
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
